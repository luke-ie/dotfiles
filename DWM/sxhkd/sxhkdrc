#terminal emulator
super + Return
	kitty
#launcher
super + Space
	rofi -show drun
#nvim
super + shift + Return
	kitty -e nvim ..
#firefox
super + shift + f
	firefox
#ranger
super + e

#Volume Control
XF86AudioLowerVolume
	amixer set Master 5%-
XF86AudioRaiseVolume
	amixer set Master 5%+
XF86AudioMute
        amixer set Master toggle	

        amixer set Master toggle
